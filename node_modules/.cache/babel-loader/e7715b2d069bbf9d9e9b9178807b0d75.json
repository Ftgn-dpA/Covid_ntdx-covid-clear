AAChBE,MAAAA,MAAM,CAACF,KAAD,CAAN;AACH,KAJD;AAKH,GANM,CAAP;AAOH","sourcesContent":["import axios from 'axios';\r\n\r\naxios.defaults.timeout = 120000;\r\n\r\n// request interceptor\r\naxios.interceptors.request.use(\r\n    config => {\r\n        return config;\r\n    },\r\n    error => {\r\n        return Promise.reject(error);\r\n    }\r\n);\r\n\r\n// response interceptor\r\naxios.interceptors.response.use(\r\n    response => response,\r\n    error => {\r\n        return Promise.reject(error);\r\n    }\r\n);\r\n\r\n/**\r\n * @method ajax\r\n * @param  {Object} parameter\r\n * @return {Object}\r\n * @example\r\n *     ajax({\r\n *         url: '/xx/xx/xx',\r\n *         method: 'get', // 默认为 'get'\r\n *         params: {    // 当 GET请求时 的时候参数放在 params,当 POST 请求时 参数放在 data\r\n *             userId: 123\r\n *         },\r\n *         timeout: 5000\r\n *         ......\r\n *     });\r\n * @public\r\n */\r\nexport default function ajax(parameter) {\r\n    return new Promise((resolve, reject) => {\r\n        axios(parameter).then((response) => {\r\n            resolve(response.data);\r\n        }).catch((error) => {\r\n            reject(error);\r\n        });\r\n    });\r\n}"]}]}